/*Botones instalacion*/
a.fb {
	background-color: rgb(59, 118, 184);
	border: medium none;
	border-radius: 4px 4px 4px 4px;
	color: rgb(255, 255, 255);
	cursor: pointer;
	float: left;
	font: bold 0.825em/1 "Helvetica","Arial",sans-serif;
	height: auto;
	letter-spacing: 0.025em;
	margin: 1em 0px 0px 1.5em;
	padding: 0.8em 25px 0.7em;
	padding-right: 5px;
	position: relative;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
	width: 15em;
}
a.website {
	background-color: rgb(73, 199, 244);
	border: medium none;
	border-radius: 4px 4px 4px 4px;
	color: rgb(255, 255, 255);
	cursor: pointer;
	float: left;
	font: bold 0.825em/1 "Helvetica","Arial",sans-serif;
	height: auto;
	letter-spacing: 0.025em;
	margin: 1em 0px 0px 1.5em;
	padding: 0.8em 25px 0.7em;
	padding-right: 5px;
	position: relative;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
	width: 15em;
}
a.fb::after{
	background-image:url('/img/facebook_install.png');
	content:"";
	height:20px;
	left:.4em;
	position:absolute;
	top:.4em;
	width:20px;
}
a.website::after{
	background-image:url('/img/website_install.png');
	content:"";
	height:20px;
	left:.4em;
	position:absolute;
	top:.4em;
	width:20px;
}

