body {
    /*background-color: #F4F4F4;*/
    background-color: #e2e2e2;
}


a{
    color: #3F1D4F;
    text-decoration: none;
}

.badge .btn,.dropdown-menu,a, .input-group{
    transition: all 0.3s ease;
}

.open>.dropdown-menu {
    display: block;
    z-index: 1031;
    margin-top: 1px;
    border-radius: 0;
    border: 1px solid #f9f9f9;
}

.container{
    padding:0;   
}

#item-container {
    position: relative;
    bottom: -1170px;
    z-index: 1000;
    height: 100%;
    left: 0;
    display: none;
    margin-top: 0px;
    margin-bottom: 25px;
}
#preview{
    cursor: -webkit-zoom-in;
}

.scrollup {
    bottom: -5px;
    height: 50px;
    line-height: 18px;
    padding: 10px 5px;
    position: fixed;
    right: 40px;
    text-align: center;
    width: 125px;
    z-index: 1100;
    color: #FFFFFF;
    background-color: #3dc5f5;
    border-color: #3dc5f5;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#6E3877', endColorstr='#3F1D4F', GradientType=0);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

.scrollup:hover{
    text-decoration:none;
    color:#ffffff;
    bottom:0px;
}

#infscr-loading {
    clear: both;
    padding: 10px 0;
    position: relative;
    width: 250px;
    left: 46.5%;
}

#loading-items {
    font-size: 16px;
    font-weight: 300;
    position: relative;
    top: -40px;
    left: -90px;
}

#loading-items-empty {
    font-size: 16px;
    font-weight: 300;
    position: relative;
    left: -90px;
    padding: 20px 0;
    text-align: center;
}

#left-loading-items {
    float: left;
}


#item-container .container{
    height: 100%;
}
#item-detail{
    display: inline-block;
    position: inherit;
    width:100%;
    background: #FFFFFF;
    padding:10px 20px;
    /*-moz-box-shadow: 0px 5px 26px #333;
    -webkit-box-shadow: 0px 5px 26px #333;
    -o-box-shadow: 0px 5px 26px #333;
    box-shadow: 0px 5px 26px #333;*/
    color:#333;
}

#item-detail .description{
    margin-bottom: 25px;
}

#item-detail .close{
    font-size: 30px;
}
#item-detail .title{
    font-family: Helvetica Narrow, sans-serif;
    margin:10px 0px;
    font-weight: 200;
    font-size: 40px;
    padding:0;
    color: #355986;
}

#item-detail .precio {
    text-align: center;
    padding-top: 90px;
    padding-bottom: 50px;
    color: #490052;
    font-weight: 200;
    font-size: 40px;
}


#menu-settings{
    position: absolute;
    top:0;
    right: 0;
    padding: 0;
    display:block;
    background: transparent;
}
#menu-settings .nav{
    background:rgba(0,0,0,0.3);
    transition: all 0.8s ease;
}
#menu-settings .nav > li > a{
    color: #efefef;
    text-transform: uppercase; 
    background: transparent !important;
}
#menu-settings .nav > li > a:hover,#menu-settings > .nav > a:focus{
    background: none;
    color:#ffffff;
}
#menu-shopping {
    height: 50px;
    background: #262935;
    margin-bottom: 10px;
    box-shadow: 0px -22px 90px rgba(0,0,0,0.25);
}

#menu-shopping .container-fluid.notheaderless .navbar-header{
    display:none;
}

.navbar>.container .navbar-brand, .navbar>.container-fluid .navbar-brand{
    margin:0;
}

ul.shopping-cart{
    min-width: 235px;
}

ul.shopping-cart li a.menuitem{
    padding: 15px;
    border-bottom: 1px solid #e5e5e5;
}

ul.shopping-cart li.subtotal{
padding: 10px 0;
color: #999;
}

ul.shopping-cart li a.menuitem .item-cart-data{
    width: 152px;
    margin: 0 5px;
    white-space: normal;
    display: inline-block;
}

ul.shopping-cart li a.menuitem .item-cart-data b{
    display: block;
    clear: right;
}

ul.shopping-cart li:hover button.fa{
    visibility: visible;
}

ul.shopping-cart li button.fa{
    position: absolute;
    right: 15px;
    visibility: hidden;
    margin-top: -30px;
    background: none;
    border: none;
    color: #000;
    cursor: pointer;
}

#menu-shopping.navbar-fixed-top{
    z-index:10036;
    left: auto;
    right: auto;
}

#menu-shopping div > ul{
    padding:0;
    margin:0;
    list-style:none;
    height:50px;
    float:right;
}
#menu-shopping div > ul > li{
    display:inline-block;
    float:left;
    padding:6px 15px;
}
#menu-shopping div > ul > li > a{
    color:#FFF;
    font-size: 26px;
    text-decoration: none;
}
#menu-shopping div > ul > li > a:hover,#menu-shopping div > ul > li > a:focus{
    text-decoration: none;
    color:#CCC;
}
#menu-shopping .badge{
    vertical-align: middle;
    background-color: #44495c;
    color: #9a79a8;
}
#menu-shopping .input-group{
    width: 200px;
    margin: 4px 0;
    background-color: #44495c;
    color: #ccc;
    border: none;
    min-height: 30px;
    border-radius: 5px;
}

#menu-shopping .input-group.focus {
    outline: none;
    -moz-box-shadow: 0px 0px 6px rgba(102, 175, 233, 0.8);
    -webkit-box-shadow: 0px 0px 6px rgba(102, 175, 233, 0.8);
    box-shadow: 0px 0px 6px rgba(102, 175, 233, 0.8);
}

#menu-shopping input{
    padding:3px 0 3px 12px;
    height: 31px;
    background: transparent;
    border: none;
    color: #e2e2e2;
}

#menu-shopping input:focus{
    outline: none;
    border:none;
    -moz-box-shadow: inset 0 0 0 rgba(0,0,0,0.0);
    -webkit-box-shadow: inset 0 0 0 rgba(0,0,0,0.0);
    box-shadow: inset 0 0 0 rgba(0,0,0,0.0);
}

#menu-shopping form button {
    background: none;
    border: none;
    color: #9a79a8;
    padding:5px 10px;
}

#header{
    background: #f8f8f8;
}
#header .image-cover{
    background-repeat: no-repeat;
    background-position: center;
    background-color: #E2E2E2;
    background-size: cover;
    height: 275px;
}
#header .page-header {
    margin-left: 200px;
    margin-top: -69px;
    color: #FFFFFF;
    text-shadow: 1px 1px 1px #333;
    position: absolute;
    border: none;
    line-height: 38px;
    z-index: 3;
    margin-bottom: 0;
    padding: 0;
}
#header .page-header h1{
    font-size: 40px;
}
#header .page-header p{
    font-size: 20px;
    font-weight: 200;
}
#header .store-logo{
    position: absolute;
    margin-left: 20px;
    margin-top: -125px;
    width: 160px;
    height: 160px;
    z-index: 2;
}
.main-container #middle{
    min-height: 910px;
    overflow:hidden;
    background: #FFFFFF;
}

.dropdown-menu:before {
    position: absolute;
    top: -7px;
    right: 10px;
    display: inline-block;
    border-right: 7px solid transparent;
    border-bottom: 7px solid #ccc;
    border-left: 7px solid transparent;
    border-bottom-color: transparent;
    content: '';
}

.dropdown-menu:after {
    position: absolute;
    top: -6px;
    right: 10px;
    display: inline-block;
    border-right: 6px solid transparent;
    border-bottom: 6px solid #ffffff;
    border-left: 6px solid transparent;
    content: '';
}

.card{
    width: 270px;
    clear: both;
    padding: 10px 20px;
}

.card .person {
    height: 110px;
    width: 110px;
    background: url(https://www.kichink.com/v3/img/como_funciona/servicio_al_cliente.png) no-repeat -61px -36px #e2e2e2;
    margin: 20px 0;
    opacity: 0.9;
    ms-filter: "progid:DXImageTransform.Microsoft.BasicImage(grayscale=1)";
    filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter ???.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale");
    filter: gray;
    -webkit-filter: grayscale(100%);
    -moz-filter: grayscale(100%);
    -ms-filter: grayscale(100%);
    -o-filter: grayscale(100%);
    animation: fadeInRotate 0.9s ease;
    -webkit-animation: fadeInRotate 0.9s ease;
    -moz-animation: fadeInRotate 0.9s ease;
    -ms-animation: fadeInRotate 0.9s ease;
    -o-animation: fadeInRotate 0.9s ease;
}
.card input {
    margin: 0;
    height: 40px;
    z-index: 1;
    font-size: 16px;
    border-radius: 0;
}
.card button{
    margin-bottom: 5px;
}

#categories{
    z-index: 1;
}
#items{
    padding: 0;
    margin: 0;
    list-style: none;
}

#items .item {
    list-style: none;
    width: 200px;
    height: 230px;
    margin: 10px;
    display: inline-block;
    border: 1px solid #D5D5D5;
    cursor: pointer;
    position:relative;
}

#items .item img{
    width: 100%;
}

.caroufred{
    margin-top:15px;
}

.caroufred .inner{
    overflow:hidden;
    width: 88%;
    display: inline-block;
    height:80px;
}
#thumbs {
    overflow: hidden;
    text-align: left;
    height:100px;
    width:100%;
    z-index:3;
}


#thumbs img {
    height: auto;
    display: block;
    margin-right: 12px;
    float: left;
    cursor:pointer;
    padding: 0 10px;
    height:70%;
}

#thumbs img.selected{
    border-color:#490052;
}

#item-detail .images{
    border: 1px solid #d5d5d5;
    margin: 15px 0;
}

.stop-scrolling {
    height: 100%;
    overflow: hidden;
}
/*
#thumbs img {
    border: 1px solid #ccc;
    padding: 5px;
    margin: 0 12px 0 12px;
    cursor: pointer;
    overflow: hidden;
    height: 80px;
}*/

#categories .active >a{
    background: none;
    color: #333;                    
}
#categories a{
    color:#999;
}

#checkout.modal .modal-dialog{
    width: 1000px;
    height: 680px;
    border:none;
    text-align: center;
    background: none;
}

#checkout.modal .modal-content{
    border:none;
    background: transparent;
    -webkit-box-shadow: 0 5px 15px rgba(0,0,0,0);
    box-shadow: 0 5px 15px rgba(0,0,0,0);
}

.navbar{
    z-index: inherit;
}
.options{
    border-bottom: 2px dotted #b4b4b4;
    padding-bottom: 20px;
    color: #b4b4b4;
}

.options .btn.btn-primary#buy_button{
    background: #3F1D4F;
    border-color: #6E3877;
    width:100%;
    margin-top:40px;
    margin-bottom:40px;
}

.options .btn.btn-primary#buy_button:hover{
    background: #542d65;
}


.options table tr td{
    padding: 10px 0;
}

.options table tr td:last-child{
    color:#656565
}

.share-controls{
    font-size: 24px;
    padding: 0 0 30px;
    margin: 25px 0;
    list-style: none;
    text-align: center;
}
.share-controls li{
    display:inline-block;
}
.share-controls li a{
    color: #333;
}            

#footer {
    color: #666666;
    text-align: left;
}

#footer .footer-data{
    background: #FFFFFF;
    padding-top: 20px;
    display:inline-block;
    width:100%;
    padding-bottom: 20px;
}

.discount .items-price{
    text-decoration: line-through;
}


.discount .items-discount{
    color: #900;
    display:inline-block;
}

.items-price {
    text-align: left;
    font-size:18px;
    background: transparent;
    font-weight: 700;
    color: inherit;
    display: block;
    padding: 0;
    position: relative;
}

.ribbon:before {
    content: '';
    position: absolute;
    border-style: solid;
    border-width: 0px 8px 8px 0px;
    border-color: #333 transparent;
    display: block;
    width: 0;
    z-index: 1;
    top: -8px;
    right: 0px;
}    

.ribbon {
    -moz-box-shadow: 2px 2px 0 rgba(0,0,0,0.1);
    -webkit-box-shadow: 2px 2px 0 rgba(0,0,0,0.1);
    -o-box-shadow: 2px 2px 0 rgba(0,0,0,0.1);
    box-shadow: 2px 2px 0 rgba(0,0,0,0.1);
    background: #683A7B;
    color: #FFFFFF;
    font-size: 13px;
    right:-8px;
    margin-top: 15px;
    min-width: 50px;
    max-width: 80px;
    z-index: 1;
    font-size: 13px;
    position: absolute;
    min-width: 50px;
    padding: 6px 7px 6px 7px;
}

.items-data{
    padding: 10px;
    float:left;
}

.items-name{
    text-align: left;
    font-weight: 200;
    font-size: 13px;
    max-height: 35px;
    overflow: hidden;
}

.nav-pills>li+li {
margin-left: 0;
}

ul.listres{
    padding:0;
}
ul.listres li {
    list-style: none;
    margin: 0px;
    padding:0;
    color: #490052;
}

.posuno {
    float: left;
    margin-top:15px;
    height: 80px;
    display: inline-block;
    z-index: 5;
}
.posdos {
    margin-top: 15px;
    display: inline-block;
    height: 80px;
    position: relative;
    margin-left: -15px;
    float: right;
    z-index: 5;
}
.posuno a, .posdos a{
    background:#999;
    padding:4px;
    text-align:center;
    width:24px;
    height:25px;
    cursor: pointer;
    color:#ffffff;
    font-size: 18px;
    display:table-cell !important;
}
.cover-bottom{
    display:none;
    height:0;
    opacity: 0;
    -webkit-filter: blur(5px);
    height: 10px;
    overflow: hidden;
    margin-top: 50px;
    position: absolute;
}
@media screen and (min-width: 767px) and (max-width: 981px){
    #header .page-header{
        margin-top: -110px;
        line-height: 20px;
    }
}

@media (max-width:480px)  {

    #middle > div{
        padding:0 7px;
    }

    /*#items .item {
        list-style: none;
        width: 120px;
        height: 170px;
        margin: 10px;
        display: inline-block;
        border: 1px solid #D5D5D5;
        cursor: pointer;
        position: relative;
    }*/
}

@media (max-width:767px)  {
    
    footer .footer-data .col-xs-6{
        display:none;
    }

    .scrollup{
        display:none !important;
    }

    #menu-shopping .dropdown-menu:before {
        visibility: hidden;
    }

    #menu-shopping .dropdown-menu:after {
        visibility: hidden;
    }

    #menu-shopping .dropdown-menu.pull-right {
        left: -40px;
    }

    #menu-shopping.navbar-fixed-top {
        left: 0;
        right: 0;
    }

    #menu-shopping .navbar-header{
        display:none;
    }

    #header .nav-pills>li, #header .nav-pills>li a {
        float: none;
        display: block;
    }

    #header .page-header{
        margin: 0;
        padding: 0;
        position: relative;
        text-align: center;
        color: #555;
        font-weight: 100;
        text-shadow: 0 0 0;
        margin-bottom: 20px;
    }
    #header .store-logo{
        margin: 0 auto;
        position: relative;
        margin-top: -80px;
    }
    #header .store-logo img{
        border-radius:50%;
    }
    #header .page-header h1{
        font-weight: 200;
    }
    #header #menu-shopping{
        margin:0;
    }

    #middle #menu-shopping form{
        display:none;
    }

    #items{
        text-align:center;
    }
    #item-container #item-detail .title {
        font-size: 22px;
    }
    .caroufred{
        display:none;
    }
}